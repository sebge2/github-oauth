{
  "/api/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/ws/*": {
    "target": "ws://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "ws": true,
    "changeOrigin": true
  },
  "/login": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/logout": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  }
}
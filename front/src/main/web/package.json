{
  "name": "i18n-tool",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test-local": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "webdriver-start": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && webdriver-manager  start --detach --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "webdriver-local-start": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && webdriver-manager  start --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "e2e-local": "ng e2e --protractorConfig=./e2e/protractor.local-conf.js",
    "generate-api": "cross-env API_URL='http://127.0.0.1:8080/api/api-docs' ts-node --project=../../../scripts/tsconfig.json ../../../scripts/generate-api.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.1.1",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@stomp/ng2-stompjs": "~7.2.0",
    "@stomp/stompjs": "~5.4.2",
    "bcp-47": "^1.0.7",
    "core-js": "^2.5.4",
    "flag-icon-css": "^3.3.0",
    "lodash": "^4.17.15",
    "mat-progress-buttons": "^8.0.7",
    "messageformat": "^2.3.0",
    "ngx-flag-icon-css": "^1.0.1",
    "ngx-translate-messageformat-compiler": "^4.5.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.136",
    "@types/node": "^8.10.61",
    "codelyzer": "^5.0.1",
    "cross-env": "^7.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "^5.4.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tlib": "^1.0.0",
    "typescript": "~3.4.5"
  }
}

{
  "name": "i18n-tool",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test-local": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "webdriver-start": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && webdriver-manager  start --detach --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "webdriver-local-start": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && webdriver-manager  start --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "e2e-local": "ng e2e --protractorConfig=./e2e/protractor.local-conf.js",
    "generate-api": "cross-env API_URL='http://127.0.0.1:8080/api/api-docs' ts-node --project=../../../scripts/tsconfig.json ../../../scripts/generate-api.ts",
    "generate-font": "icon-font-generator ./src/app-fonts/*.svg -o ./src/assets/fonts -p app-icon -n app-icons --tag \"*\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.1.3",
    "@angular/compiler": "^8.1.3",
    "@angular/core": "^8.1.3",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.1.3",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.1.3",
    "@angular/platform-browser-dynamic": "^8.1.3",
    "@angular/router": "^8.1.3",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@stomp/ng2-stompjs": "~7.2.0",
    "@stomp/stompjs": "^5.4.4",
    "bcp-47": "^1.0.7",
    "core-js": "^2.6.11",
    "flag-icon-css": "^3.5.0",
    "lodash": "^4.17.19",
    "mat-progress-buttons": "^8.0.8",
    "messageformat": "^2.3.0",
    "ngx-flag-icon-css": "^1.0.1",
    "ngx-translate-messageformat-compiler": "^4.8.0",
    "rxjs": "^6.5.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular/cli": "^8.1.3",
    "@angular/compiler-cli": "^8.1.3",
    "@angular/language-service": "^8.1.3",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@types/jasmine": "^2.8.17",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.158",
    "@types/node": "^8.10.62",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.0",
    "icon-font-generator": "^2.1.10",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "^5.4.4",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~7.0.0",
    "tslib": "^1.13.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}

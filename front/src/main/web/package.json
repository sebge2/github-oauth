{
  "name": "i18n-tool",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "postinstall": "ngcc",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test-local": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "webdriver-start": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && webdriver-manager  start --detach --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "webdriver-local-start": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && webdriver-manager  start --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "e2e-local": "ng e2e --protractorConfig=./e2e/protractor.local-conf.js",
    "generate-api": "cross-env API_URL='http://127.0.0.1:8080/api/api-docs' ts-node --project=../../../scripts/tsconfig.json ../../../scripts/generate-api.ts",
    "generate-font": "fantasticon ./src/app-fonts/ -o ./src/assets/fonts -p app-icon -n app-icons --tag \"*\"",
    "format-all": "prettier --write .",
    "format-staged": "pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bcp-47": "^2.1.0",
    "core-js": "^3.20.0",
    "flag-icon-css": "^4.1.7",
    "lodash": "^4.17.21",
    "mat-progress-buttons": "^9.3.1",
    "messageformat": "^2.3.0",
    "ng-dynamic-component": "^10.1.0",
    "ngx-clipboard": "^15.0.1",
    "ngx-flag-icon-css": "^1.0.1",
    "ngx-translate-messageformat-compiler": "^4.11.0",
    "rxjs": "^7.4.0",
    "safe-pipe": "^1.0.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.10",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@angular/language-service": "~13.1.1",
    "@angular/localize": "^13.1.1",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.1",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.3",
    "fantasticon": "^1.2.3",
    "git-format-staged": "^2.1.3",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-spec-reporter": "^0.0.32",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "protractor": "~7.0.0",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "ts-node": "~10.4.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.4"
  }
}

{
    "AWSEBDockerrunVersion": "1",
    "Image": {
        "Name": "@docker.repository@/@docker.image.name@:@docker.image.version@",
        "Update": "true"
    },
    "Ports": [
        {
            "ContainerPort": 8080,
            "HostPort": 8000
        }
    ],
    "Volumes": [
        {
            "HostDirectory": "/media/ebs_volume",
            "ContainerDirectory": "/srv"
        }
    ],
    "Logging": "/var/log"
}
